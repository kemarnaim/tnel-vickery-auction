<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ei-config SYSTEM "../../ei-config.dtd">
<ei-config>
<jade-parameters>
<parameter>local-port=2011</parameter>
</jade-parameters>

<institutional-agents>

<agent name="rma">
<class>jade.tools.rma.rma</class>
</agent>

<agent name="Bidder">
	<class>VickreyBidder</class> 
	<argument>role=IR-enterprise-agent</argument>
</agent>
<agent name="Auctioneer">
		<class>VickreyNegotiationFacilitator</class>
		<argument>service=IS-negotiation-facilitator</argument>
</agent>

<agent name="Seller">
		<class>VickreySeller</class>
		<argument>role=IR-enterprise-agent</argument>
		
</agent>

</institutional-agents>
</ei-config>
